{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run ExampleProject-ExampleApp",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/combaltaExampleApp/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/ExampleApp",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/ExampleProject/ExampleApp\"}"
      },
      "envFile": "${workspaceFolder}/ExampleApp/.env1",
      "preLaunchTask": "Build ExampleProject"
    }
  ]
}